{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "text"
    },
    "Image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true
    },
    "Expired": {
      "type": "date",
      "required": false
    }
  }
}
